{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"watch-sass": "sass --watch src/sass:_site/css",
		"serve-eleventy": "eleventy --serve",
		"build-sass": "sass --no-source-map src/sass:_site/css",
		"build-eleventy": "eleventy",
		"postbuild": "postcss _site/css/**/*.css -u autoprefixer cssnano -r --no-map",
		"start": "concurrently -r \"npm:watch-sass\" \"npm:serve-eleventy\"",
		"build": "concurrently \"build-sass\" \"build-eleventy\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HarshaVardhanNakkina/portfolio.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/HarshaVardhanNakkina/portfolio/issues"
	},
	"homepage": "https://github.com/HarshaVardhanNakkina/portfolio#readme",
	"devDependencies": {
		"@11ty/eleventy": "^0.12.1",
		"autoprefixer": "^10.2.5",
		"concurrently": "^6.0.2",
		"cssnano": "^5.0.1",
		"postcss": "^8.2.13",
		"postcss-cli": "^8.3.1",
		"sass": "^1.32.11"
	}
}
